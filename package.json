{
	"name": "svelte-storage",
	"version": "0.2.2",
	"homepage": "https://github.com/7korobi/svelte-storage/blob/main/src/lib/",
	"license": "MIT",
	"keywords": [
		"svelte",
		"component",
		"storage"
	],
	"author": {
		"name": "7korobi",
		"email": "7korobi@gmail.com"
	},
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && svelte-package",
		"publish:npm": "cd package && npm publish",
		"publish": "run-s format build publish:npm",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@playwright/test": "^1.27.0",
		"@sveltejs/adapter-auto": "1.0.0-next.80",
		"@sveltejs/kit": "1.0.0-next.511",
		"@sveltejs/package": "1.0.0-next.5",
		"devalue": "^4.0.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.7.1",
		"svelte": "^3.50.1",
		"svelte-check": "^2.9.1",
		"svelte-preprocess": "^4.10.7",
		"typescript": "^4.8.4",
		"vite": "^3.1.6"
	},
	"dependencies": {
		"date-fns": "^2.29.3",
		"dexie": "^3.2.2",
		"ioredis": "^5.2.3",
		"svelte-browser": "^0.3.21",
		"svelte-petit-utils": "^0.2.1",
		"svelte-tick-timer": "^0.1.4",
		"tslib": "^2.4.0"
	}
}
